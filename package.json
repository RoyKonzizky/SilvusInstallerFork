{
  "name": "Silvus",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "out/main/main.cjs",
  "scripts": {
    "start": "npm run build && npm run electron:start",
    "dev": "concurrently -k \"npm run vite\" \"npm run electron:dev\"",
    "vite": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "cross-env IS_DEV=true electron .",
    "electron:build": "electron-builder",
    "electron:package": "electron-forge package",
    "package": "electron-packager . Silvus --platform=win32 --arch=x64 --out=.\\dist --overwrite"
  },
  "dependencies": {
    "@antv/graphin": "^2.7.27",
    "@reduxjs/toolkit": "^2.2.3",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "antd": "^5.18.1",
    "axios": "^1.7.2",
    "child_process": "^1.0.2",
    "electron-builder": "^24.13.3",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hyper-modal": "^1.2.16",
    "react-i18next": "^14.1.2",
    "react-modal": "^3.16.1",
    "react-redux": "^9.1.1",
    "react-router-dom": "^6.22.3",
    "react-select": "^5.8.0",
    "react-use-websocket": "^4.8.1",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-modal": "^3.16.3",
    "@types/styled-react-modal": "^1.2.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.3.0",
    "electron-packager": "^17.1.2",
    "electron-vite": "^2.2.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  },
  "build": {
    "appId": "com.silvus.id",
    "productName": "Silvus",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "main.js",
      "preload.js",
      "package.json",
      "start-app-environment.ps1",
      "Silvus.bat"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
